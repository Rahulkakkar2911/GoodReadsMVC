@model IEnumerable<LibraryManagement.Models.RahulBooking>
<br/>
@if(Model.Count() != 0){
<table class ="table">
    <tr>
        <th>
            Book Name
        </th>
        <th>
            Return
        </th>
    </tr>

    
        @foreach(var item in Model){
            <tr>
                <td>@item.BidNavigation.BookName</td>
                <td>
                    <form asp-action="Delete" asp-controller = "Booking" asp-route-id = "@item.Bookingid" Method="POST">
                        <div class="form-group">
                            <input type="submit" value="Return" class="btn btn-primary" />
                        </div>
                    </form>
                </td>
            </tr>
        }
    
</table>
}
else{
    <h2 class="text-primary">No Bookings Found!</h2>
}